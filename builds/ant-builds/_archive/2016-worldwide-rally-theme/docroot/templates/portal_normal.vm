				</div>
            </div><!-- //section content -->
        </section><!-- //about content-->
		
		<!-- news content-->
        <section id="blog" class="page_section sectionWithFill">
            <!-- section header -->
			<div class="container">
				<h2 class="whiteTxt">News</h2>
			</div><!-- //section header -->
            
